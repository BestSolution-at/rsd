{
  "name": "@bestsolution/rsd-webcomponents",
  "version": "0.0.1",
  "description": "Webcomponents to work with RSD",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/rsd-viewer/rsd-viewer.esm.js",
  "exports": {
    ".": {
      "import": "./dist/rsd-viewer/rsd-viewer.esm.js",
      "require": "./dist/rsd-viewer/rsd-viewer.cjs.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BestSolution-at/rsd.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "tw-css": "npx @tailwindcss/cli -i ./dummy.css -o ./src/components/rsd-viewer/rsd-viewer.css --watch"
  },
  "devDependencies": {
    "@stencil/core": "^4.7.0",
    "@tailwindcss/cli": "^4.1.3",
    "@tailwindcss/typography": "^0.5.16",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^21.9.0",
    "tailwindcss": "^4.1.3"
  },
  "license": "MIT"
}
